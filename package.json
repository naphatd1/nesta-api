{
  "name": "nest-auth-api",
  "version": "1.0.0",
  "description": "NestJS API with Authentication and Authorization",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:devs": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:build": "chmod +x scripts/docker-build.sh && ./scripts/docker-build.sh",
    "docker:dev": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh",
    "docker:prod": "chmod +x scripts/docker-prod.sh && ./scripts/docker-prod.sh",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod:up": "docker-compose up -d",
    "docker:prod:down": "docker-compose down",
    "test:api": "node test-api.js",
    "test:api:auth": "node test-api.js auth",
    "test:api:upload": "node test-api.js upload",
    "test:api:monitoring": "node test-api.js monitoring",
    "test:api:health": "node test-api.js health",
    "check:server": "node check-server.js",
    "wait:server": "node check-server.js wait",
    "dev:test": "npm run start:dev & npm run wait:server && npm run test:api",
    "test:full": "./run-tests.sh all",
    "test:auth:full": "./run-tests.sh auth",
    "test:upload:full": "./run-tests.sh upload",
    "test:monitoring:full": "./run-tests.sh monitoring",
    "test:health:full": "./run-tests.sh health"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.12.0",
    "@supabase/supabase-js": "^2.53.0",
    "argon2": "^0.43.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "newman": "^6.2.1",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
